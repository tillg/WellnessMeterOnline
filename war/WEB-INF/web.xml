<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>WellnessMeterOnline</servlet-name>
		<servlet-class>com.tillg.wellnessmeter.WellnessMeterOnlineServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WellnessMeterOnline</servlet-name>
		<url-pattern>/wellnessmeteronline</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

  <servlet>
    <servlet-name>AddWeight</servlet-name>
    <servlet-class>com.tillg.wellnessmeter.AddWeight</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddWeight</servlet-name>
    <url-pattern>/AddWeight</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>AddMeasurement</servlet-name>
    <servlet-class>com.tillg.wellnessmeter.AddMeasurement</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddMeasurement</servlet-name>
    <url-pattern>/AddMeasurement</url-pattern>
  </servlet-mapping>

</web-app>
